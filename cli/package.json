{
  "name": "handy-cli",
  "description": "A new CLI generated with oclif",
  "version": "0.0.0",
  "author": "Kirill Dubovitskiy",
  "bin": {
    "handy-cli": "./bin/run.js"
  },
  "bugs": "https://github.com/bra1ndump/handy-cli/issues",
  "dependencies": {
    "@oclif/core": "4.4.1",
    "@oclif/plugin-help": "^6",
    "axios": "^1.10.0",
    "chalk": "^5.4.1",
    "dotenv": "^17.2.0",
    "qrcode-terminal": "^0.12.0",
    "socket.io-client": "^4.8.1",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/qrcode-terminal": "^0.12.2",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/bra1ndump/handy-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "handy-cli",
    "dirname": "handy-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": "bra1ndump/handy-cli",
  "imports": {
    "#auth/*": "./dist/auth/*.js",
    "#claude/*": "./dist/claude/*.js",
    "#commands/*": "./dist/commands/*.js",
    "#handlers/*": "./dist/handlers/*.js",
    "#session/*": "./dist/session/*.js",
    "#socket/*": "./dist/socket/*.js",
    "#utils/*": "./dist/utils/*.js"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "npm run build && vitest run",
    "test:watch": "vitest",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
